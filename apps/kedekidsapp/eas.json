{
  "cli": {
    "version": ">= 16.10.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "env:GOOGLE_SERVICES_JSON"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "hooks": {
    "prebuild": "node scripts/setup-google-services.js"
  }
}
